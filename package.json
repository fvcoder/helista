{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all --serial  \"build\"",
    "build:tailwind": "tailwindcss -i app/lib/ui/tailwind.css -o app/styles/tailwind.build.css --watch",
    "build:remix": "remix build",
    "dev": "npm-run-all --parallel \"dev:*\"",
    "dev:remix": "remix watch",
    "dev:tailwind": "tailwindcss -i app/lib/ui/tailwind.css -o app/styles/tailwind.build.css --watch",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.js --watch ./server.js",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.14",
    "@radix-ui/colors": "^0.1.8",
    "@remix-run/express": "^1.14.0",
    "@remix-run/node": "^1.14.0",
    "@remix-run/react": "^1.14.0",
    "@stitches/react": "^1.2.8",
    "@types/socket.io": "^3.0.2",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.5",
    "morgan": "^1.10.0",
    "peer": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.14.0",
    "@remix-run/eslint-config": "^1.14.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-codely": "^2.1.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
